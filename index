    <script>
      function getQuestions(input) {
        const questions = input.split("#done").map((q, index) => {
          const lines = q.trim().split("\n");
          return {
            number: index + parseInt(startNumberInput.value),
            text: lines[0] || "",
            choices: lines.slice(1),
          };
        });
        return questions;
      }
      function parseQuestions() {
        // Get the user input from the textarea
        const input = document.getElementById("questionInput").value;
        const yearInput = document.getElementById("yearInput").value;
        const startNumberInput =
          document.getElementById("startNumberInput").value;
        const styleInput = document.getElementById("styleInput").value;
        const questionType = document.getElementById("questionType").value;

        // Split the input by '#done' to get each question
        const questions = getQuestions(input);

        // Get the container where questions will be displayed
        const container = document.getElementById("target-html-container");
        container.innerHTML = ""; // Clear previous results

        const boxes = document.querySelectorAll(".box");
        // Change background based on selected style
        if (styleInput === "bacteria") {
          document.body.classList.add("bacteria-background");
          // Add a new class to each '.box' element
          //   boxes.forEach((box) => {
          //     box.classList.add("bacteria-background"); // Replace 'new-class' with the name of the class you want to add
          //   });
        } else {
          document.body.classList.remove("bacteria-background");
          //   boxes.forEach((box) => {
          //     box.classList.remove("bacteria-background"); // Replace 'new-class' with the name of the class you want to add
          //   });
        }

        // Loop through questions array and create HTML structure for each question
        questions.forEach((question, index) => {
          // Create the HTML structure using a template literal

          console.log(question);
          const questionHTML = `
          <div class="box">
            <div class="icons">
              <div class="icon">
                <img src="https://f.top4top.io/p_31411f48l1.png" alt="Icon 1" />
              </div>
              <div class="icon">
                <img src="https://g.top4top.io/p_3132vv89s1.png" alt="Icon 2" />
              </div>
              <div class="icon">${question?.number}</div>
              <!-- Question number -->
            </div>
            <div class="content">
              <p class="text">
                <strong>${question?.text}</strong>
              </p>
              <ul class="text">${question.choices
                .map((choice) => `<li>${choice}</li>`)
                .join("")}
             </ul>
            </div>
            
            <div class="triangle"></div>
            <h2>${questionType}</h2>
            
            <div class="number">${yearInput}</div>
            <!-- Year -->
          </div>
        `;

          // Insert the question HTML into the container
          container.innerHTML += questionHTML;
        });
      }

      function downloadHTML() {
        const input = document.getElementById("questionInput").value;
        document.getElementById("questionInput").defaultValue = input;
        console.log("ss", input);
        const blob = new Blob([document.documentElement.outerHTML], {
          type: "text/html",
        });
        const url = URL.createObjectURL(blob);
        const link = document.createElement("a");
        link.href = url;
        link.download = "questions.html";
        link.click();
      }
    </script>
  

</body></html>
